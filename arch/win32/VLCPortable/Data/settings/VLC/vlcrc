#dvb-inversion=2
# Satellite Polarisation (Chaîne)
#dvb-polarisation=
# Network Identifier (Entier)
#dvb-network-id=0
# Satellite Azimuth (Entier)
#dvb-azimuth=0
# Satellite Elevation (Entier)
#dvb-elevation=0
# Satellite Longitude (Entier)
#dvb-longitude=0
# Satellite Range Code (Chaîne)
#dvb-range=
# Antenna lnb_lof1 (kHz) (Entier)
#dvb-lnb-lof1=0
# Antenna lnb_lof2 (kHz) (Entier)
#dvb-lnb-lof2=0
# Antenna lnb_slof (kHz) (Entier)
#dvb-lnb-slof=0
# Transponder symbol rate in kHz (Entier)
#dvb-srate=27500
# Modulation type (Entier)
#dvb-modulation=-1
# ATSC Major Channel (Entier)
#dvb-major-channel=0
# ATSC Minor Channel (Entier)
#dvb-minor-channel=0
# ATSC Physical Channel (Entier)
#dvb-physical-channel=0
# FEC rate (Entier)
#dvb-code-rate-hp=-1
# Terrestrial low priority stream code rate (FEC) (Entier)
#dvb-code-rate-lp=-1
# Terrestrial bandwidth (Entier)
#dvb-bandwidth=0
# Terrestrial guard interval (Entier)
#dvb-guard=-1
# Terrestrial transmission mode (Entier)
#dvb-transmission=-1
# Terrestrial hierarchy mode (Entier)
#dvb-hierarchy=-1
