[vout_sdl] # Simple DirectMedia Layer video output
# SDL chroma format (Chaîne)
#sdl-chroma=
[portaudio] # PORTAUDIO audio output
# Output device (Entier)
#portaudio-audio-device=0
[oldtelnet] # VLM remote control interface
# Host (Chaîne)
#telnet-host=localhost
# Port (Entier)
#telnet-port=4212
# Password (Chaîne)
#telnet-password=admin
[oldhttp] # HTTP remote control interface
# Host address (Chaîne)
#http-host=
# Source directory (Chaîne)
#http-src=
# Handlers (Chaîne)
#http-handlers=
# Export album art as /art (Booléen)
#http-album-art=0
# Certificate file (Chaîne)
#http-intf-cert=
# Private key file (Chaîne)
#http-intf-key=
# Root CA file (Chaîne)
#http-intf-ca=
# CRL file (Chaîne)
#http-intf-crl=
[invmem] # Memory video decoder
# Width (Entier)
#invmem-width=0
# Height (Entier)
#invmem-height=0
# Lock function (Chaîne)
#invmem-lock=0
# Unlock function (Chaîne)
#invmem-unlock=0
# Callback data (Chaîne)
#invmem-data=0
# Chroma (Chaîne)
#invmem-chroma=RV24
[fake] # Fake video decoder
# Image file (Chaîne)
#fake-file=
# Reload image file (Entier)
#fake-file-reload=0
# Video width (Entier)
#fake-width=0
# Video height (Entier)
#fake-height=0
# Keep aspect ratio (Booléen)
#fake-keep-ar=0
# Background aspect ratio (Chaîne)
#fake-aspect-ratio=
# Deinterlace video (Booléen)
#fake-deinterlace=0
# Deinterlace module (Chaîne)
#fake-deinterlace-module=deinterlace
# Chroma used (Chaîne)
#fake-chroma=I420
[crop] # Crop video filter
# Crop geometry (pixels) (Chaîne)
#crop-geometry=
# Automatic cropping (Booléen)
#autocrop=0
# Ratio max (x 1000) (Entier)
#autocrop-ratio-max=2405
# Manual ratio (Entier)
#crop-ratio=0
# Number of images for change (Entier)
#autocrop-time=25
# Number of lines for change (Entier)
#autocrop-diff=16
# Number of non black pixels  (Entier)
#autocrop-non-black-pixels=3
# Skip percentage (%) (Entier)
#autocrop-skip-percent=17
# Luminance threshold  (Entier)
#autocrop-luminance-threshold=40
[bda] # DirectShow DVB input
# Caching value in ms (Entier)
#dvb-caching=300
# Transponder/multiplex frequency (Entier)
#dvb-frequency=0
# Network Name (Chaîne)
#dvb-network-name=
